<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
	<listeners>
		<listener class-name="org.testng.reporters.EmailableReporter2" />
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	<test name="FireFox_Driver">
		<parameter name="desiredCapabilites" value="desiredcapabilities.properties" />
		<parameter name="profilePreferences" value="profileperferences.properties" />
		<parameter name="firebugPreferences" value="firebugpreferences.properties" />
		<classes>
			<class
				name="com.kineticskunk.auto.webdriverfactory.FireFoxDriverFactoryTestNG" />
		</classes>
	</test>
	<test name="Chrome_Driver">
		<classes>
			<class
				name="com.kineticskunk.auto.webdriverfactory.ChromeDriverFactoryTestNG" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->